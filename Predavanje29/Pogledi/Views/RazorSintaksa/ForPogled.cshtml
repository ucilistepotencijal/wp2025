@model string[]
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Razor Sintaksa - For</title>
</head>
<body>
    <div>
        <ul>
            @for (int i = 0; i < Model.Length; i++)
            {
                <li>@Model[i]</li>
            }
        </ul>
    </div>
</body>
</html>
