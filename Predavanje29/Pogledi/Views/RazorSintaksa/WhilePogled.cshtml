@model string[]
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
    <head>
    <meta charset="utf-8" />
    <title>Razor Sintaksa - While</title>
</head>
<body>
    <div>
        <ul>
            @{
                int i = 0;
                while (i < Model.Length)
                {
                    <li>@Model[i]</li>
                    i++;
                }
            }
        </ul>
    </div>
</body>
</html>
